@model IEnumerable<InterGalacticEcomm.Models.Category>

<ul>
    @foreach (var item in Model)
    {
        <li>
            <a asp-controller="Home" asp-action="Category" asp-route-CategoryName="@item.CategoryName" asp-route-Description="@item.Description">@item.CategoryName</a>
        </li>
    }
</ul>